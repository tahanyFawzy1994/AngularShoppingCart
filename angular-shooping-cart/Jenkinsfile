pipeline {

    agent any
    tools {nodejs “node”}
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                dir 'angular-shooping-cart'
                bat 'npm install'
            }
        }
        stage('Run') {
            steps {
                echo 'Running...'
                dir 'angular-shooping-cart'
                bat 'npm start'
            }
        }
    }
}
