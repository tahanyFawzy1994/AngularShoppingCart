pipeline {

    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
              sh 'cd angular-shooping-cart'  
              sh 'npm install'
            }
        }
        stage('Run') {
            steps {
                echo 'Running...'
              sh 'cd angular-shooping-cart'
                sh 'npm start'
            }
        }
    }
}
