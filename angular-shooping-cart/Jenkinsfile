pipeline {

    agent any
    tools {nodejs "node"}
    stages {
      stage ('Move to'){
        steps{
        dir 'angular-shooping-cart'
        }
      }
        stage('Build') {
            steps {
                echo 'Building...'
                bat 'npm install'
            }
		}
          stage('Run') {
            steps {
                echo 'Running...'
                bat 'npm start'
            }
        
        }
    }
}
