pipeline {

    agent any
    stages {
      stage('move'){
      steps {
                echo 'Moving...'
              bat 'cd angular-shooping-cart'
            }
      }
        stage('Build') {
            steps {
                echo 'Building...'
              bat 'npm install'
            }
        }
        stage('Run') {
            steps {
                echo 'Running...'
                bat 'npm start'
            }
        }
    }
}
